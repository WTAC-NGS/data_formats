
        set terminal pdf size 3.5,2.5
        set output "indel-cycles.pdf"
        set style line 1 linetype 1  lw 4 linecolor rgb "#337ab7"
        set style line 2 linetype 2  lw 4 linecolor rgb "#5cb85c"
        set style line 3 linetype 3  lw 4 linecolor rgb "#f0ad4e"
        set style line 4 linetype 4  lw 4 linecolor rgb "#d9534f"
        set style increment user
        set ylabel "Indel count"
        set xlabel "Read Cycle"
        set format y "%.0g"
        set key left
    plot '-' w l ti 'Insertions (fwd)', '' w l ti 'Insertions (rev)', '' w l ti 'Deletions (fwd)', '' w l ti 'Deletions (rev)'
